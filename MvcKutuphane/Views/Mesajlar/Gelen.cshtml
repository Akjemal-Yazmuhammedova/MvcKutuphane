@using MvcKutuphane.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Gelen";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}


<h2>Gelen Mesajlar Sayfası</h2>
<br />
<table class="table table-hover">
    <tr>
        <th>Göderen</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>İşlem</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.GONDEREN</td>
            <td>@x.KONU</td>
            <td>@x.ICERIK</td>
            <td>@x.TARIH</td>
        </tr>
    }
</table>


